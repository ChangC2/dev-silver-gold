<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_margin="@dimen/margin_5"
    android:padding="@dimen/margin_5">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:id="@+id/lytPostGroup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                android:visibility="gone"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_menu_posts_buying_group" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/posts_group"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytPostSupplier"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                android:visibility="gone"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_menu_post_suppliers" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/posts_supplier"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytProduct"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_product"
                        app:tint="@color/md_grey_800" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/products"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytBrand"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_feature_store"
                        app:tint="@color/md_grey_800" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/txt_brands"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytFollower"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_menu_followers" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/followers"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytVendor"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_menu_post_suppliers" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/txt_vendors"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytUserManagement"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_menu_user_management" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/user_management"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytUserProfile"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_menu_user_profile" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/user_profile"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytSupplierProfile"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_menu_store_profiles" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/txt_supplier_profile"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytBankDetail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_menu_payment" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/bank_detail"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/lytPayment"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/margin_10"
                app:cardElevation="1dp"
                app:cardUseCompatPadding="true"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/margin_5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_15">

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/ic_menu_payment" />

                    <TextView
                        style="@style/TextStyleNormalRegular"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:text="@string/payment"
                        android:layout_gravity="center"
                        android:gravity="center"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <TextView
                style="@style/TextStyleNormalRegular"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginTop="@dimen/margin_20"
                android:layout_marginBottom="@dimen/margin_20"
                android:text="Version 1.0.0"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</LinearLayout>